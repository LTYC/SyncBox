{
	"name": "LTYC/SyncBox",
	"description": "The SyncBox Application.",
	"keywords": ["sync", "box", "syncbox", "dropbox"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "~5.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0"
	},
	"autoload": {
		"classmap": [
			"database",
			"tests/TestCase.php"
		],
		"psr-4": {
			"SyncBox\\": "app/"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan route:scan",
			"php artisan event:scan",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
